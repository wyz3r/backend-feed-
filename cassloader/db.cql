CREATE KEYSPACE IF NOT EXISTS dlrfeed WITH replication = {'class': 'SimpleStrategy', 'replication_factor':1};
/*  CREATE KEYSPACE IF NOT EXISTS dlrfeed WITH replication = {'class': 'NetworkTopologyStrategy', 'DLRMAXCOM': '3'}  AND durable_writes = true; */
CREATE TABLE dlrfeed.projects (
    project_id timeuuid,
    project_name varchar,
    marcas varchar,
    categoria varchar,
    archive BOOLEAN,
    PRIMARY KEY (project_id)
);
