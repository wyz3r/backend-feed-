CREATE TABLE dlrfeed.user_projects (
    user_id text,
    project_id timeuuid,
    PRIMARY KEY (user_id, project_id)
);

CREATE TABLE dlrfeed.project_stimulus (
    project_id timeuuid,
    estimulo_id varchar,
    PRIMARY KEY (project_id, estimulo_id)
);

-- ya sea que el estimulo se ¬°jale de el backend o se guarde en un serverless 
CREATE TABLE dlrfeed.poll (
    estimulo_id varchar,
    name varchar,
    description varchar,
    logo varchar, 
    title varchar,
    answers text,
    tipos varchar,
    type_poll varchar,
    PRIMARY KEY (  estimulo_id)
);

